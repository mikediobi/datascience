    "from sklearn.linear_model import LinearRegression\n",
    "lr = LinearRegression()\n",
    "\n",
    "# Create X and y.\n",
    "featured = ['average_price', 'small', 'x_large']\n",
    "X = avocado[featured]\n",
    "y = avocado.total_sales\n",
    "\n",
    "X_train, X_test, y_train, y_test = train_test_split(X,y, train_size = .75) #order matters\n",
    "\n",
    "# Instantiate and fit.\n",
